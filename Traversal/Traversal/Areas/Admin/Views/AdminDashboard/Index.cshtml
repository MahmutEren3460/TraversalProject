@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<!DOCTYPE html>
<html lang="tr"
	  class="light-style layout-menu-fixed"
	  dir="ltr"
	  data-theme="theme-default"
	  data-assets-path=".../assets/"
	  data-template="vertical-menu-template-free">
<body>
		<div class="row">
			@await Component.InvokeAsync("DashboardBanner")
			@await Component.InvokeAsync("Cards1Statistic")
			@await Component.InvokeAsync("TotalRevenue")
			@await Component.InvokeAsync("Cards2Statistics")			
		</div>
		<div class="row">
		@await Component.InvokeAsync("DestinationStatistic")
		@await Component.InvokeAsync("DestinationStatistic2")
		@await Component.InvokeAsync("AdminGuideList")			
	</div>
	<div class="content-backdrop fade"></div>
	<div class="layout-overlay layout-menu-toggle"></div>
</body>
</html>


